---
custom_basename: dashboard
---
<div class="container-fluid">
  <div class="row row--main">
    <div class="col-lg-6 col-xl-3">
      <div class="boxes">
        <div class="row">
          <div class="col-4">
            <div class="circle circle--border circle--success">
              <svg class="svg-icon">
                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#cloud"></use>
              </svg>
            </div>
          </div>
          <div class="col-8 v-align">
            <h5>62,510</h5>
            <p>{{dashboard.stats.visits}}</p>
          </div>
        </div>
      </div>
    </div>
    <div class="col-lg-6 col-xl-3">
      <div class="boxes">
        <div class="row">
          <div class="col-4">
            <div class="circle circle--border circle--primary">
              <svg class="svg-icon">
                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#monetization"></use>
              </svg>
            </div>
          </div>
          <div class="col-8 v-align">
            <h5>62,510</h5>
            <p>{{dashboard.stats.revenue}}</p>
          </div>
        </div>
      </div>
    </div>
    <div class="col-lg-6 col-xl-3">
      <div class="boxes">
        <div class="row">
          <div class="col-4">
            <div class="circle circle--border circle--danger">
              <svg class="svg-icon">
                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#favorite"></use>
              </svg>
            </div>
          </div>
          <div class="col-8 v-align">
            <h5>62,510</h5>
            <p>{{dashboard.stats.sales}}</p>
          </div>
        </div>
      </div>
    </div>

    <div class="col-lg-6 col-xl-3">
      <div class="boxes">
        <div class="row">
          <div class="col-4">
            <div class="circle circle--border circle--secondary">
              <svg class="svg-icon">
                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#char"></use>
              </svg>
            </div>
          </div>
          <div class="col-8 v-align">
            <h5>62,510</h5>
            <p>{{dashboard.stats.conversion}}</p>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="row row--main">
    <div class="col-lg-12 col-xl-4">
      <div class="boxes">
        {{dashboard.charts.y_report}}
        <canvas id="myChart1"></canvas>
      </div>
    </div>
    <div class="col-lg-12 col-xl-4">
      <div class="boxes">
        {{dashboard.charts.revenue}}
        <canvas id="myChart2"></canvas>
      </div>
    </div>
    <div class="col-lg-12 col-xl-4">
      <div class="boxes">
        {{dashboard.charts.revenue}}
        <canvas id="myChart3"></canvas>
      </div>
    </div>
  </div>
  <div class="row row--main">
    <div class="col-lg-12 col-xl-8">
      <div class="boxes">
        <h6 class="boxes__title">{{dashboard.table.project}}</h6>
        <p class="boxes__subheading">{{dashboard.table.text}}</p>
        <table class="table">
          <thead>
            <tr>
              <th scope="col">#</th>
              <th scope="col">First</th>
              <th scope="col">Last</th>
              <th scope="col">Handle</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <th scope="row">1</th>
              <td>Mark</td>
              <td>Otto</td>
              <td>@mdo</td>
            </tr>
            <tr>
              <th scope="row">2</th>
              <td>Jacob</td>
              <td>Thornton</td>
              <td>@fat</td>
            </tr>
            <tr>
              <th scope="row">3</th>
              <td>Larry</td>
              <td>the Bird</td>
              <td>@twitter</td>
            </tr>
          </tbody>
        </table>
      </div>
    </div>
    <div class="col-lg-8 col-xl-4">
      <div class="row row--users">
        <div class="col-12">
          <div class="boxes">
            <h6>Vardas</h6>
            <p>vardas@pavarde.com</p>
            <p>{{dashboard.users.admin}}</p>
          </div>
        </div>
      </div>
      <div class="row row--users">
        <div class="col-12">
          <div class="boxes">
            <h6>Vardas</h6>
            <p>vardas@pavarde.com</p>
            <p>{{dashboard.users.user}}</p>
          </div>
        </div>
      </div>
      <div class="row row--users">
        <div class="col-12">
          <div class="boxes">
            <h6>Vardas</h6>
            <p>vardas@pavarde.com</p>
            <p>{{dashboard.users.moderator}}</p>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
